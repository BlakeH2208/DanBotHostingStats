<%- include("./partials/header.ejs", { botInfo: false, Title: "Home", Avatar: "", Redirect: "/me" }) %>

<meta http-equiv="refresh" content="10"/>
<br><br><br>

<br><br><br>

<main>

    <div class="container bg-light text-dark">
        <table class="table table-striped">
            <thead>
            <tr>
                <th>CPU Load</th>
                <th>RAM Usage</th>
                <th>SSD Usage</th>
                <th>NET Usage</th>
                <th>Server Count</th>
            </tr>
            </thead>
            <tbody>
            <%table.forEach(function(entry) {%>
                <tr>
                    <td> <%= entry.info.servername %> </td>
                    <td> <%= entry.cpu_percent.toFixed(2) %>% </td>
                    <td><%- pretty(entry.mem_usage) %> </td>
                    <td>IN: <%- pretty(entry.netIO.rx)%>, OUT: <%- pretty(entry.netIO.tx) %></td>
                    <td><a style="color:red;" href="https://panel.danbot.host/server/<%=entry.name.slice(0, -28)%>">View Server</a>, <a style="color:blue;" href="https://panel.danbot.host/server/<%=entry.name.slice(0, -28)%>/files">Server Files</a></td>

                </tr>
            <%});%>
            </tbody>
        </table>
    </div>

</main>


    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
<script>
document.onkeydown = function(e) {
        if (e.ctrlKey &&
            (e.keyCode === 67 ||
             e.keyCode === 86 ||
             e.keyCode === 85 ||
             e.keyCode === 117)) {
            return false;
        } else {
            return true;
        }
};
$(document).keypress("u",function(e) {
  if(e.ctrlKey)
  {
return false;
}
else
{
return true;
}
});
</script>
